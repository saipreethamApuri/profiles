{
  "routes": [
    {
      "route": "/remoteEntry.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/media/*.{png,jpg,jpeg,gif,bmp}", "/static/css/*"]
  },
  "mimeTypes": {
    ".js": "application/javascript"
  }
} 